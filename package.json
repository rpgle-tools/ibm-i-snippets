{
  "name": "snippets-for-ibm-i-languages",
  "displayName": "Snippets for IBM i languages",
  "publisher": "CristianRivera",
  "description": "todo",
  "version": "0.0.15",
  "icon": "assets/images/snippets-for-ibm-i-languages.jpg",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onCommand:cspec.showCSpec"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snippets-for-ibm-i-languages.showCSpec",
        "title": "Create C Spec for RPG Fixed format"
      }
    ],
    "snippets": [
      {
        "language": "rpgle",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "rpg",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "sqlrpgle",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "dependencies": {
    "npm-watch": "^0.11.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
